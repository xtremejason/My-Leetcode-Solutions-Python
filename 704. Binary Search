class Solution(object):
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        
        low, mid = 0, 0
        high = len(nums) - 1
        
        while(low <= high):
            mid = low + (high - low)
            if (nums[mid] == target): 
                return mid
            elif (nums[mid] < target):
                low = mid + 1
            else: 
                high = mid - 1
            
        return -1
        
        
